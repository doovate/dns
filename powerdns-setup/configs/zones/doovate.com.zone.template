$TTL 3600
@   IN SOA dns.${DNS_ZONE}. admin.${DNS_ZONE}. (
        2025010101 ; serial
        3600       ; refresh
        900        ; retry
        604800     ; expire
        86400 )    ; minimum

    IN NS dns.${DNS_ZONE}.

dns IN A ${DNS_SERVER_IP}
# Additional A records will be added by step 06
